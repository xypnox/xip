{
  "name": "@xypnox/xip-ui",
  "version": "0.1.3",
  "description": "A UI library for SolidJS with Themescura",
  "source": "src/index.tsx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xypnox/xip.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "author": "xypnox",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/xypnox/xip/issues"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "solid": {
        "default": "./dist/source/index.jsx",
        "types": "./dist/types/index.d.ts"
      },
      "import": {
        "default": "./dist/esm/index.js",
        "types": "./dist/types/index.d.ts"
      },
      "browser": {
        "default": "./dist/esm/index.js",
        "types": "./dist/types/index.d.ts"
      },
      "require": {
        "default": "./dist/cjs/index.js",
        "types": "./dist/types/index.d.ts"
      },
      "node": {
        "default": "./dist/cjs/index.js",
        "types": "./dist/types/index.d.ts"
      }
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "solidjs",
    "ui-components",
    "css-modules",
    "solidjs-components",
    "component-library",
    "user-interface",
    "design-system",
    "web-ui",
    "ui-framework",
    "solidjs-ui",
    "frontend-library",
    "interactive-elements"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup --watch -c",
    "doc": "api-extractor run --local --verbose"
  },
  "homepage": "https://github.com/xypnox/xip#readme",
  "peerDependencies": {
    "@xypnox/themescura": "workspace:*",
    "solid-js": "^1.5.4"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.47.7",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/node": "^18.7.16",
    "postcss-modules": "^6.0.0",
    "prettier": "^2.7.1",
    "rollup": "^2.75.7",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-preset-solid": "^1.4.0",
    "solid-js": "^1.4.2",
    "typescript": "^4.6.4",
    "typescript-plugin-css-modules": "^5.1.0"
  }
}
