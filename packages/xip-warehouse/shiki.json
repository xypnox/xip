{
  "name": "xipDark",
  "settings": [
    {
      "settings": {
        "foreground": "var(--text)",
        "background": "var(--surface-0)"
      }
    },
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "header",
      "settings": {
        "foreground": "var(--primary-s-1)"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "var(--textS-0)"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "var(--primary-s-2)"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "var(--primary-s-2)"
      }
    },
    {
      "scope": "constant.regexp",
      "settings": {
        "foreground": "var(--primary-s-4)"
      }
    },
    {
      "scope": [
        "constant.other.color.rgb-value.css",
        "constant.numeric.color.rgb-value.scss",
        "constant.other.rgb-value.css"
      ],
      "settings": {
        "foreground": "var(--primary-s-4)"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "var(--secondary-s-5)"
      }
    },
    {
      "scope": "entity.name.tag.css",
      "settings": {
        "foreground": "var(--secondary-s-5)"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "var(--tertiary-s-5)"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.class.mixin.css",
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.parent-selector.css",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css",
        "source.css.less entity.other.attribute-name.id",
        "entity.other.attribute-name.attribute.scss",
        "entity.other.attribute-name.scss"
      ],
      "settings": {
        "foreground": "var(--tertiary-s-5)"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "var(--tertiary-s-3)"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "var(--primary-s-2)"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "fontStyle": "bold",
        "foreground": "var(--primary-s-2)"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "var(--primary-s-5)"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "var(--primary-s-5)"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "var(--primary-s-5)"
      }
    },
    {
      "scope": "beginning.punctuation.definition.quote.markdown",
      "settings": {
        "foreground": "var(--secondary-s-6)"
      }
    },
    {
      "scope": "beginning.punctuation.definition.list.markdown",
      "settings": {
        "foreground": "var(--secondary-s-6)"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "var(--tertiary-s-5)"
      }
    },
    {
      "scope": "meta.selector",
      "settings": {
        "foreground": "var(--tertiary-s-5)"
      }
    },
    {
      "scope": "brackets of XML/HTML tags",
      "settings": {
        "foreground": "var(--secondary-s-6)"
      }
    },
    {
      "scope": "meta.preprocessor",
      "settings": {
        "foreground": "var(--primary-s-5)"
      }
    },
    {
      "scope": "meta.preprocessor.string",
      "settings": {
        "foreground": "var(--primary-s-5)"
      }
    },
    {
      "scope": "meta.preprocessor.numeric",
      "settings": {
        "foreground": "var(--primary-s-5)"
      }
    },
    {
      "scope": "meta.structure.dictionary.key.python",
      "settings": {
        "foreground": "var(--primary-s-5)"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "var(--primary-s-5)"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "var(--secondary-s-5)"
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "foreground": "var(--secondary-s-5)"
      }
    },
    {
      "scope": "storage.modifier",
      "settings": {
        "foreground": "var(--secondary-s-5)"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "var(--primary-s-5)"
      }
    },
    {
      "scope": "string.tag",
      "settings": {
        "foreground": "var(--primary-s-5)"
      }
    },
    {
      "scope": "string.value",
      "settings": {
        "foreground": "var(--primary-s-5)"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "var(--secondary-s-5)"
      }
    },
    {
      "scope": [
        "punctuation.definition.template-expression.begin.ts",
        "punctuation.definition.template-expression.end.ts"
      ],
      "settings": {
        "foreground": "var(--primary-s-5)"
      }
    },
    {
      "scope": "support.type.property-name",
      "settings": {
        "foreground": "var(--primary-s-4)"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "var(--primary-s-5)"
      }
    },
    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "var(--primary-s-5)"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "var(--primary-s-4)"
      }
    },
    {
      "scope": ["keyword.operator.new", "keyword.operator.expression"],
      "settings": {
        "foreground": "var(--primary-s-5)"
      }
    },
    {
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "var(--primary-s-5)"
      }
    },
    {
      "scope": [
        "punctuation.section.embedded.metatag.begin.php",
        "punctuation.section.embedded.metatag.end.php"
      ],
      "settings": {
        "foreground": "var(--primary-s-6)"
      }
    },
    {
      "scope": "support.function.git-rebase",
      "settings": {
        "foreground": "var(--primary-s-6)"
      }
    },
    {
      "scope": "constant.sha.git-rebase",
      "settings": {
        "foreground": "var(--primary-s-6)"
      }
    },
    {
      "name": "coloring of the Java import and package identifiers",
      "scope": [
        "storage.modifier.import.java",
        "storage.modifier.package.java"
      ],
      "settings": {
        "foreground": "var(--primary-s-6)"
      }
    },
    {
      "name": "this.self",
      "scope": "variable.language",
      "settings": {
        "foreground": "var(--secondary-s-6)"
      }
    },
    {
      "name": "Function declarations",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "var(--primary-s-4)"
      }
    },
    {
      "name": "Types declaration and references",
      "scope": [
        "support.class",
        "entity.name.class",
        "storage.type.cs",
        "storage.type.generic.cs",
        "storage.type.modifier.cs",
        "storage.type.variable.cs",
        "storage.type.annotation.java",
        "storage.type.generic.java",
        "storage.type.java",
        "storage.type.object.array.java",
        "storage.type.primitive.array.java",
        "storage.type.primitive.java",
        "storage.type.token.java",
        "storage.type.groovy",
        "storage.type.annotation.groovy",
        "storage.type.parameters.groovy",
        "storage.type.generic.groovy",
        "storage.type.object.array.groovy",
        "storage.type.primitive.array.groovy",
        "storage.type.primitive.groovy"
      ],
      "settings": {
        "foreground": "var(--primary-s-6)"
      }
    },
    {
      "name": "Types declaration and references, TS grammar specific",
      "scope": [
        "meta.return-type",
        "support.type",
        "entity.name.type",
        "meta.return.type",
        "meta.type.cast.expr",
        "meta.type.new.expr",
        "support.constant.math",
        "support.constant.dom",
        "support.constant.json"
      ],
      "settings": {
        "foreground": "var(--secondary-s-5)"
      }
    },
    {
      "scope": ["meta.definition.variable.name", "support.variable"],
      "settings": {
        "foreground": "var(--tertiary-s-5)"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "var(--tertiary-s-5)"
      }
    },
    {
      "name": "Variable and parameter name",
      "scope": ["variable"],
      "settings": {
        "foreground": "var(--tertiary-s-4)"
      }
    },
    {
      "name": "Object keys, TS grammar specific",
      "scope": [
        "meta.object-literal.key",
        "meta.object-literal.key entity.name.function"
      ],
      "settings": {
        "foreground": "var(--text)"
      }
    },
    {
      "name": "CSS property value",
      "scope": [
        "constant.other.color.rgb-value.css",
        "constant.other.rgb-value.css",
        "meta.property-value.css support.function",
        "meta.property-value.css support",
        "constant.numeric.color.rgb-value.scss",
        "constant.rgb-value.scss",
        "meta.property-value.scss support.function",
        "meta.property-value.scss support"
      ],
      "settings": {
        "foreground": "var(--primary-s-5)"
      }
    }
  ]
}
